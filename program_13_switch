public class program_13_Switch
{
    public static void main(String[]arg)
    {
        int x=3;
        switch (x)
        {
            case 1:
                System.out.print("Sun day");
                break;
            case 2:
                System.out.print("Mon day");
                break;
            case 3:
                System.out.print("Tues day");
                break;
            case 4:
                System.out.print("Wednesday");
                break;
            case 5:
                System.out.print("Thurs day");
                break;
            case 6:
                System.out.print("Fri day");
                break;
            case 7:
                System.out.print("Saturday");
        }
    }
}
