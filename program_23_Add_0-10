public class program_26_Add_0_10
{
    public static void main(String[]arg)
    {
        int result= sum(10);
        System.out.println(result);
    }
    public static int sum(int x)
    {
        if(x>0)
        {
            return x+sum(x-1);
        }
        else
        {
           return 0;
        }
    }
}
